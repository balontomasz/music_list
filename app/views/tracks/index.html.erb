

<div class="row">
  <ol>
    <% @tracks.each do |track| %>
      <div class="col-md-4">
        <div class="card">
          <%= embed(track.link) %>
          <div class="card-block">
            <div class="btn-group">
              <%= link_to "A", "#", class: 'btn btn-success' %>
              <%= link_to "A", "#", class: 'btn btn-info' %>
              <%= link_to "X", "#", class: 'btn btn-danger' %>
            </div>
          </div>
        </div>
      </div>
    <% end %>
  </ol>
</div>
